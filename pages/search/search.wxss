.header{
  display: flex;
  width: 100%;
  padding: 22rpx 23rpx;
  box-sizing: border-box;
  box-shadow:0px 4px 10px 0px rgba(53,53,53,0.04);
  align-items: center;
}
.header .b-sea{
display: flex;
width:618rpx;
height:64rpx;
background:rgba(240,240,244,1);
border-radius:32rpx;
align-items: center;
}
.header .fan-h{
font-size:30rpx;
font-family:PingFang;
color:rgba(118,124,140,1);
margin-left: 28rpx;
}
.title{
  width: 100%;
  padding: 41rpx 25rpx 13rpx;
  box-sizing: border-box;
  display: flex;
}
.title .left{
width: 50%;
font-size:30rpx;
font-family:PingFang;
color:rgba(59,64,78,1);
}
.title .right{
  width: 50%;
  font-size:30rpx;
  font-family:PingFang;
  color:#a5aab8;
  display: flex;
  justify-content: flex-end;
}
.list{
  padding: 0 25rpx;
  width: 100%;
  box-sizing: border-box;
}
.list .hospital{
  width: 100%;
  display: flex;
  height: 90rpx;
  border-bottom: 1rpx solid #f6f6f6;
  align-items: center;
}
.list .hospital .name{
  width: 50%;
  color: #a5aab8;
  font-size: 28rpx;
  font-family:PingFang;
}
.list .hospital .o-cancel{
width: 50%;
display:flex;
justify-content: flex-end;
}